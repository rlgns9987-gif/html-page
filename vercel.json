{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/privacy",
      "dest": "app.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "app.js"
    },
    {
      "src": "/styles.css",
      "dest": "/public/styles.css"
    },
    {
      "src": "/index.js",
      "dest": "/public/index.js"
    },
    {
      "src": "/(.*\\.(png|jpg|jpeg|gif|svg|ico|webp|txt|xml))",
      "dest": "/public/$1"
    },
    {
      "src": "/admin(.*)?",
      "dest": "app.js"
    },
    {
      "src": "/",
      "dest": "app.js"
    }
  ]
}